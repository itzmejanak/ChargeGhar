components:
  schemas:
    AchievementClaimData:
      type: object
      properties:
        achievement:
          $ref: '#/components/schemas/UserAchievement'
        points_awarded:
          type: integer
        new_rank:
          type: integer
          nullable: true
        total_points:
          type: integer
      required:
      - achievement
      - new_rank
      - points_awarded
      - total_points
    ActionEnum:
      enum:
      - APPROVE
      - REJECT
      type: string
      description: |-
        * `APPROVE` - APPROVE
        * `REJECT` - REJECT
    ActiveCouponsResponse:
      type: object
      description: Response serializer for active coupons endpoint
      properties:
        success:
          type: boolean
        message:
          type: string
        data:
          type: array
          items:
            $ref: '#/components/schemas/CouponPublic'
        timestamp:
          type: string
          format: date-time
      required:
      - data
      - message
      - success
      - timestamp
    AddUserBalanceRequest:
      type: object
      description: Serializer for adding user balance
      properties:
        amount:
          type: string
          format: decimal
          pattern: ^-?\d{0,8}(?:\.\d{0,2})?$
        reason:
          type: string
          minLength: 1
          maxLength: 500
      required:
      - amount
      - reason
    AdminLoginRequest:
      type: object
      description: Serializer for admin login
      properties:
        email:
          type: string
          format: email
          minLength: 1
        password:
          type: string
          writeOnly: true
          minLength: 1
      required:
      - email
      - password
    AdminProfileCreateRequest:
      type: object
      description: Serializer for creating admin profiles
      properties:
        user:
          type: integer
        role:
          $ref: '#/components/schemas/RoleEnum'
        password:
          type: string
          writeOnly: true
          minLength: 8
          maxLength: 128
      required:
      - password
      - role
      - user
    AppConfigAdminRequest:
      type: object
      description: Admin serializer for app configs (shows sensitive data to admins)
      properties:
        key:
          type: string
          minLength: 1
          maxLength: 100
        value:
          type: string
          minLength: 1
        description:
          type: string
          nullable: true
          maxLength: 255
        is_active:
          type: boolean
      required:
      - key
      - value
    AppUpdateRequest:
      type: object
      description: Serializer for AppUpdate model
      properties:
        version:
          type: string
          minLength: 1
          maxLength: 255
        title:
          type: string
          minLength: 1
          maxLength: 255
        description:
          type: string
          minLength: 1
        features: {}
        is_major:
          type: boolean
        released_at:
          type: string
          format: date-time
      required:
      - description
      - released_at
      - title
      - version
    AppVersionCheckRequest:
      type: object
      description: Serializer for app version check requests
      properties:
        platform:
          $ref: '#/components/schemas/PlatformEnum'
        current_version:
          type: string
          minLength: 1
          maxLength: 255
      required:
      - current_version
      - platform
    AppVersionCheckResponse:
      type: object
      description: Response serializer for version check
      properties:
        update_available:
          type: boolean
        is_mandatory:
          type: boolean
        latest_version:
          type: string
        download_url:
          type: string
          format: uri
          nullable: true
        release_notes:
          type: string
          nullable: true
        current_version:
          type: string
      required:
      - current_version
      - download_url
      - is_mandatory
      - latest_version
      - release_notes
      - update_available
    AppVersionRequest:
      type: object
      description: Serializer for AppVersion model
      properties:
        version:
          type: string
          minLength: 1
          maxLength: 255
        platform:
          $ref: '#/components/schemas/PlatformEnum'
        is_mandatory:
          type: boolean
        download_url:
          type: string
          format: uri
          minLength: 1
          maxLength: 200
        release_notes:
          type: string
          minLength: 1
        released_at:
          type: string
          format: date-time
      required:
      - download_url
      - platform
      - release_notes
      - released_at
      - version
    AuthCompleteRequest:
      type: object
      description: Serializer for authentication completion
      properties:
        identifier:
          type: string
          minLength: 1
          description: Email or phone number
        verification_token:
          type: string
          format: uuid
          description: Token from OTP verification
        username:
          type: string
          minLength: 1
          description: Required only for new user registration
          maxLength: 150
      required:
      - identifier
      - verification_token
    BannerRequest:
      type: object
      description: Serializer for banners
      properties:
        title:
          type: string
          minLength: 1
          maxLength: 255
        description:
          type: string
          nullable: true
          maxLength: 255
        image_url:
          type: string
          format: uri
          minLength: 1
          maxLength: 200
        redirect_url:
          type: string
          format: uri
          nullable: true
          maxLength: 200
        display_order:
          type: integer
          maximum: 2147483647
          minimum: -2147483648
        is_active:
          type: boolean
        valid_from:
          type: string
          format: date-time
        valid_until:
          type: string
          format: date-time
      required:
      - image_url
      - title
      - valid_from
      - valid_until
    BaseResponse:
      type: object
      description: Standard response format for all API endpoints
      properties:
        success:
          type: boolean
        message:
          type: string
        data: {}
        timestamp:
          type: string
          format: date-time
      required:
      - data
      - message
      - success
      - timestamp
    BroadcastMessageRequest:
      type: object
      description: Serializer for broadcasting messages
      properties:
        title:
          type: string
          minLength: 1
          maxLength: 200
        message:
          type: string
          minLength: 1
          maxLength: 1000
        target_audience:
          $ref: '#/components/schemas/TargetAudienceEnum'
        send_push:
          type: boolean
          default: true
        send_email:
          type: boolean
          default: false
      required:
      - message
      - target_audience
      - title
    BulkClaimData:
      type: object
      properties:
        claimed_count:
          type: integer
        failed_count:
          type: integer
        total_points_awarded:
          type: integer
        achievements:
          type: array
          items:
            $ref: '#/components/schemas/UserAchievement'
        failed_claims:
          type: array
          items:
            $ref: '#/components/schemas/FailedClaim'
      required:
      - achievements
      - claimed_count
      - failed_claims
      - failed_count
      - total_points_awarded
    BulkClaimRequest:
      type: object
      description: Serializer for bulk achievement claiming
      properties:
        achievement_ids:
          type: array
          items:
            type: string
            format: uuid
          description: List of UserAchievement IDs (not Achievement IDs) to claim.
            These are the IDs from the achievements endpoint.
          maxItems: 50
          minItems: 1
      required:
      - achievement_ids
    BulkClaimResponse:
      type: object
      description: Response serializer for bulk claim endpoint
      properties:
        success:
          type: boolean
        message:
          type: string
        data:
          $ref: '#/components/schemas/BulkClaimData'
        timestamp:
          type: string
          format: date-time
      required:
      - data
      - message
      - success
      - timestamp
    BulkCreateCouponRequest:
      type: object
      description: Serializer for bulk creating coupons
      properties:
        name_prefix:
          type: string
          minLength: 1
          maxLength: 100
        points_value:
          type: integer
          minimum: 1
        max_uses_per_user:
          type: integer
          minimum: 1
          default: 1
        valid_from:
          type: string
          format: date-time
        valid_until:
          type: string
          format: date-time
        quantity:
          type: integer
          maximum: 1000
          minimum: 1
        code_length:
          type: integer
          maximum: 20
          minimum: 6
          default: 8
      required:
      - name_prefix
      - points_value
      - quantity
      - valid_from
      - valid_until
    CalculatePaymentOptionsRequest:
      type: object
      description: Serializer for calculating payment options for rental scenarios
      properties:
        scenario:
          $ref: '#/components/schemas/CalculatePaymentOptionsScenarioEnum'
        package_id:
          type: string
          format: uuid
          nullable: true
        rental_id:
          type: string
          format: uuid
          nullable: true
      required:
      - scenario
    CalculatePaymentOptionsScenarioEnum:
      enum:
      - pre_payment
      - post_payment
      type: string
      description: |-
        * `pre_payment` - Pre-payment - For PREPAID rental packages
        * `post_payment` - Post-payment - For clearing outstanding rental charges
    ChannelEnum:
      enum:
      - in_app
      - push
      - sms
      - email
      type: string
      description: |-
        * `in_app` - In App
        * `push` - Push
        * `sms` - SMS
        * `email` - Email
    ClaimAchievementResponse:
      type: object
      description: Response serializer for achievement claim endpoint
      properties:
        success:
          type: boolean
        message:
