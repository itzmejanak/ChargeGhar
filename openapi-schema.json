  /api/internal/stations/data:
    post:
      operationId: internal_station_data_sync
      description: Internal endpoint for receiving station data from Java IoT management
        system. Requires admin authentication and signature validation.
      summary: Sync Station Data from IoT System
      tags:
      - Internal - IoT Integration
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                type:
                  type: string
                  enum:
                  - full
                  - returned
                  description: Type of data sync operation
                timestamp:
                  type: integer
                  description: Unix timestamp of the request
                device:
                  type: object
                  description: Device information from IoT system
                station:
                  type: object
                  description: Station configuration data
                slots:
                  type: array
                  description: Array of slot data
                power_banks:
                  type: array
                  description: Array of powerbank data
              required:
              - type
              - timestamp
              - device
      security:
      - cookieAuth: []
      - basicAuth: []
      - jwtAuth: []
      responses:
        '200':
          content:
            application/json:
              schema:
                type: object
                properties:
                  success:
                    type: boolean
                  message:
                    type: string
                  data:
                    type: object
                    properties:
                      station_id:
                        type: string
                      slots_updated:
                        type: integer
                      powerbanks_updated:
                        type: integer
                      timestamp:
                        type: string
          description: ''
        '400':
          content:
            application/json:
              schema:
                type: object
                properties:
                  success:
                    type: boolean
                  message:
                    type: string
                  error:
                    type: string
          description: ''
        '403':
          content:
            application/json:
              schema:
                type: object
                properties:
                  success:
                    type: boolean
                  message:
                    type: string
                  error:
                    type: string
          description: ''
